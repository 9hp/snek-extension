{
	"manifest_version": 2,

	"name": "circle",
	"description": "Circle extension",
	"version": "0.1.0",

	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"icons": {
		"128": "icon.png"
	},
	"background": {
		"scripts": ["event.js"],
		"persistent": false
	},

	"permissions": [
		"https://*.reddit.com/",
		"storage"
	]
}
